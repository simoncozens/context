{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "target": "es2022",
    "module": "esnext",
    "moduleResolution": "bundler",
    "lib": ["es2022", "dom"],
    "strict": false,
    "skipLibCheck": true,
    "paths": {
      "*": ["./js/*"]
    }
  },
  "include": ["js/**/*.js", "js/index.d.ts"],
  "exclude": ["node_modules", "wasm-dist", "wheels"]
}
